{
  "compilerOptions": {
    "downlevelIteration": true,
    "jsx": "react",
    "lib": ["DOM", "ES2016", "ESNext"],
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./dist/",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "ES5",
    "allowSyntheticDefaultImports": true
  }
}
